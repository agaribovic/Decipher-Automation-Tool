<text 
  label="SQ06_OTHER"
  size="40">
  <title>Please name the other institution where you see most of your patients.</title>
  <comment><span style="color:#BA0000; font-style: normal;" id="error">Please fill in the missing info.</span></comment>
  <style name="question.after" wrap="ready"><![CDATA[
// Show Other (Please specify) question

$ ('#question_SQ06_OTHER').hide();

$ ('#question_SQ06').find('select').change(function() {

    let getVal = $ ('#question_SQ06:last').find('select').val();
    
    if (getVal == 114) {
        $ ('#question_SQ06_OTHER').show(1000);

    } else {
        $ ('#question_SQ06_OTHER').hide(1000);
        $ ('#question_SQ06_OTHER').find('input').val("");
        $ ('#error').hide();
        $ ('#question_SQ06_OTHER').css('background', 'white');
    }
});

// Validation

$ ('#error').hide();

$ ("#btn_continue").click(function() {

    if ($ ('#question_SQ06_OTHER').css('display') == 'block' && !$ ('#question_SQ06_OTHER').find('input').val()) {
        $ ('#error').show();
        $ ('#btn_continue').prop('disabled', true);
        $ ('#question_SQ06_OTHER').css('background', '#FFEDED');

    }

    setTimeout(function() {
        $ ('#btn_continue').prop('disabled', false);

    }, 2000);
});
]]></style>
</text>

